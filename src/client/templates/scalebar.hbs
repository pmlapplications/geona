<img class="geona-layers-list__item-scalebar-img" src="{{scalebarData.scalebar.src}}" alt="{{t 'menu:layers-panel.scalebar.alt-text.intro'}}
  {{scalebarData.scale.identifier}} {{t 'menu:layers-panel.scalebar.alt-text.between'}}
  {{scalebarData.scale.min}} {{t 'menu:layers-panel.scalebar.alt-text.minimum'}}
  {{scalebarData.scale.max}} {{t 'menu:layers-panel.scalebar.alt-text.maximum'}}
