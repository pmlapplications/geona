  <span class="geona-layers-list__item-scalebar-labels__label centermin" title="{{scalebarData.labels.[1].real}}">{{scalebarData.labels.[1].standardForm}}</span>
  <span class="geona-layers-list__item-scalebar-labels__label center" title="{{scalebarData.labels.[2].real}}">{{scalebarData.labels.[2].standardForm}}</span>
  <span class="geona-layers-list__item-scalebar-labels__label centermax" title="{{scalebarData.labels.[3].real}}">{{scalebarData.labels.[3].standardForm}}</span>
  <span class="geona-layers-list__item-scalebar-labels__label max" title="{{scalebarData.labels.[4].real}}">{{scalebarData.labels.[4].standardForm}}</span>
