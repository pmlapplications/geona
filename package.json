{
  "name": "geona",
  "version": "0.0.1",
  "description": "Visualisation and analysis of geospatial data",
  "homepage": "https://geona.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/pmlapplications/geona"
  },
  "nyc": {
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "**/node_modules/**",
      "src/client/vendor/**",
      "src/client/templates/**",
      "src/server/vendor/**",
      "static/**",
      "test_dependencies/**"
    ],
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false,
    "cache": true,
    "all": true
  },
  "scripts": {
    "build": "grunt",
    "clean": "grunt clean",
    "start": "npm run -s serve",
    "serve": "node lib/server/app.js",
    "clientDev": "grunt clientDev",
    "serverDev": "BABEL_ENV=server  nodemon -w src/server -w src/common -w config -w locales  src/server/app.js --exec babel-node",
    "buildServerDev": "nodemon -w src/server -w src/common --exec \"grunt server && npm run serve\"",
    "test": "BABEL_ENV=test nyc mocha",
    "eslint-fix": "grunt eslint:fix"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-plugin-dynamic-import-node": "^1.0.2",
    "babel-plugin-istanbul": "^4.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.4.0",
    "babel-register": "^6.24.1",
    "babelify": "^7.3.0",
    "chai": "^4.1.0",
    "chai-http": "^3.0.0",
    "deamdify": "^0.3.0",
    "eslint": "^4.2.0",
    "eslint-config-google": "^0.9.1",
    "grunt": "^1.0.1",
    "grunt-babel": "^6.0.0",
    "grunt-browserify": "^5.0.0",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-contrib-sass": "^1.0.0",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-env": "^0.4.4",
    "grunt-eslint": "^20.0.0",
    "grunt-jsdoc": "2.1.0",
    "load-grunt-tasks": "^3.5.2",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "npm-check": "^5.4.4",
    "nyc": "^11.2.1"
  },
  "dependencies": {
    "babel-runtime": "^6.23.0",
    "bootstrap-sass": "^3.3.7",
    "chalk": "^2.0.1",
    "convict": "^4.0.0",
    "eventemitter3": "^2.0.3",
    "express": "^4.15.2",
    "grunt-handlebars-compiler": "^0.3.1",
    "hbs": "^4.0.1",
    "i18next": "^8.4.3",
    "i18next-browser-languagedetector": "^2.0.0",
    "i18next-express-middleware": "^1.0.5",
    "i18next-node-fs-backend": "^1.0.0",
    "i18next-xhr-backend": "^1.4.2",
    "jquery": "^3.2.1",
    "jquery-ui": "^1.12.1",
    "jsonix": "^2.4.1",
    "leaflet": "^1.1.0",
    "lodash": "^4.17.4",
    "ogc-schemas": "^2.6.1",
    "openlayers": "^4.3.1",
    "request": "^2.81.0",
    "swagger-jsdoc": "^1.9.6",
    "w3c-schemas": "^1.4.0"
  }
}
